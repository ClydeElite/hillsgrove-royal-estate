datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ContactInquiry {
  id        String   @id @default(cuid())
  name      String
  email     String
  phone     String
  message   String
  ip        String?
  userAgent String?
  createdAt DateTime @default(now())
}
